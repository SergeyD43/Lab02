<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:include schemaLocation="word.xsd"/>

    <xs:element name="wordList">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="word" type="word" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

</xs:schema>